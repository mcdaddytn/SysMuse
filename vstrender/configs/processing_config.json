{
  "input_file": "/path/to/input/audio.wav",
  "output_file": "/path/to/output/processed_audio.wav",
  "sample_rate": 44100,
  "buffer_size": 512,
  "plugins": [
    {
      "path": "/path/to/plugins/EQ.vst3",
      "preset": "/path/to/presets/EQ_Bright.vstpreset",
      "parameters": {
        "Low Gain": 0.2,
        "Mid Gain": 0.1,
        "High Gain": 0.3
      }
    },
    {
      "path": "/path/to/plugins/Compressor.vst3",
      "parameters": {
        "Threshold": -12.0,
        "Ratio": 4.0,
        "Attack": 10.0,
        "Release": 100.0,
        "Makeup Gain": 2.0
      }
    },
    {
      "path": "/path/to/plugins/Reverb.vst3",
      "preset": "/path/to/presets/Hall_Reverb.vstpreset",
      "parameters": {
        "Room Size": 0.7,
        "Damping": 0.5,
        "Wet Level": 0.3,
        "Dry Level": 0.8
      }
    }
  ]
}