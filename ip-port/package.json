{
  "name": "ip-port",
  "version": "1.0.0",
  "description": "TypeScript clients for USPTO Patent Data APIs (PatentsView, File Wrapper, PTAB)",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/api/server.ts",
    "test:patentsview": "tsx examples/test-patentsview.ts",
    "test:filewrapper": "tsx examples/test-file-wrapper.ts",
    "test:ptab": "tsx examples/test-ptab.ts",
    "test:all": "npm run test:patentsview && npm run test:filewrapper && npm run test:ptab",
    "example:analysis": "tsx examples/comprehensive-patent-analysis.ts",
    "example:broadcom": "tsx examples/broadcom-portfolio-builder.ts",
    "download:streaming": "tsx examples/streaming-video-downloader.ts",
    "download:streaming:test": "tsx examples/streaming-video-downloader.ts --limit 100",
    "analyze:phase1": "tsx examples/phase1-citation-analysis.ts",
    "analyze:overlap": "tsx examples/citation-overlap-analysis.ts",
    "analyze:overlap:batch": "tsx examples/citation-overlap-batch.ts",
    "analyze:overlap:highcite": "tsx examples/citation-overlap-high-cite.ts",
    "analyze:cpc": "tsx examples/cpc-overlap-analysis.ts",
    "analyze:multiscore": "tsx examples/multi-score-analysis.ts",
    "download:vmware": "tsx scripts/download-vmware-patents.ts",
    "analyze:vmware:citations": "tsx scripts/citation-overlap-vmware.ts",
    "analyze:vmware:resume": "tsx scripts/citation-overlap-vmware.ts --resume",
    "analyze:vmware:chunked": "tsx scripts/citation-overlap-vmware-chunked.ts",
    "analyze:vmware:llm-follow": "tsx scripts/vmware-llm-follower.ts",
    "merge:vmware:chunks": "tsx scripts/merge-vmware-chunks.ts",
    "merge:vmware": "tsx scripts/merge-vmware-data.ts",
    "merge:llm": "tsx scripts/merge-llm-into-analysis.ts",
    "merge:sectors": "tsx scripts/merge-sectors-into-analysis.ts",
    "merge:all": "npm run merge:llm && npm run merge:sectors",
    "download:bluecoat": "tsx scripts/download-bluecoat-patents.ts",
    "build": "tsc",
    "docker:up": "docker compose up -d postgres elasticsearch",
    "docker:up:all": "docker compose --profile dev up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "cache:stats": "tsx services/cache-service.ts stats",
    "cache:sync": "tsx services/cache-service.ts sync",
    "cache:test": "tsx scripts/test-cached-clients.ts",
    "analyze:cached": "tsx scripts/citation-overlap-cached.ts",
    "analyze:cached:dry": "tsx scripts/citation-overlap-cached.ts --dry-run",
    "download:portfolio": "tsx scripts/download-full-portfolio.ts",
    "download:portfolio:test": "tsx scripts/download-full-portfolio.ts --limit 100",
    "es:health": "tsx services/elasticsearch-service.ts health",
    "es:create-index": "tsx services/elasticsearch-service.ts create-index",
    "es:stats": "tsx services/elasticsearch-service.ts stats",
    "es:import": "tsx services/import-to-elasticsearch.ts --all",
    "es:import:priority": "tsx services/import-to-elasticsearch.ts --priority",
    "es:import:portfolio": "tsx services/import-to-elasticsearch.ts --portfolio",
    "search": "tsx services/search-cli.ts",
    "llm:test": "tsx services/llm-patent-analysis.ts test",
    "llm:analyze": "tsx services/llm-patent-analysis.ts analyze",
    "llm:batch": "tsx services/llm-patent-analysis.ts batch",
    "combine:status": "tsx services/combine-results.ts status",
    "combine:run": "tsx services/combine-results.ts combine",
    "combine:export": "tsx services/combine-results.ts export all",
    "export:llm": "tsx examples/export-for-llm.ts",
    "run:overnight": "tsx scripts/run-overnight-analysis.ts",
    "run:overnight:dry": "tsx scripts/run-overnight-analysis.ts --dry-run",
    "analyze:claims": "tsx scripts/analyze-patent-breadth.ts",
    "topRated:v2": "tsx scripts/calculate-unified-top250-v2.ts",
    "topRated:v3": "tsx scripts/calculate-and-export-v3.ts",
    "export:csv": "tsx scripts/export-raw-metrics-csv.ts",
    "export:withinsector": "tsx scripts/export-within-sector-for-excel.ts",
    "watchlist:report": "tsx scripts/manage-citator-watchlist.ts --report",
    "watchlist:update": "tsx scripts/manage-citator-watchlist.ts --update",
    "export:attorney": "tsx scripts/export-attorney-portfolio.ts",
    "export:summaries": "tsx scripts/export-summaries.ts",
    "export:all": "npm run export:attorney && npm run export:summaries && npm run topRated:v3 && npm run export:withinsector",
    "validate:portfolio": "tsx scripts/validate-portfolio-config.ts",
    "enrich:ipr": "tsx scripts/check-ipr-risk.ts",
    "enrich:ipr:top500": "tsx scripts/check-ipr-risk.ts --top 500 --skip-existing",
    "enrich:prosecution": "tsx scripts/check-prosecution-history.ts",
    "enrich:prosecution:top500": "tsx scripts/check-prosecution-history.ts --top 500 --skip-existing",
    "enrich:citations": "tsx scripts/enrich-citations.ts",
    "enrich:classify": "tsx scripts/classify-citations.ts",
    "enrich:llm": "tsx scripts/run-llm-top-patents.ts",
    "api:dev": "tsx watch src/api/server.ts",
    "api:start": "tsx src/api/server.ts"
  },
  "keywords": [
    "uspto",
    "patents",
    "patentsview",
    "file-wrapper",
    "ptab",
    "ipr",
    "patent-prosecution",
    "patent-data",
    "api-client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^1.3.10",
    "@langchain/core": "^1.1.15",
    "@prisma/client": "^5.7.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fast-xml-parser": "^5.3.4",
    "langchain": "^1.2.10",
    "node-fetch": "^3.3.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.0",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
