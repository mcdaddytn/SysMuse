{
  "name": "ip-port",
  "version": "1.0.0",
  "description": "TypeScript clients for USPTO Patent Data APIs (PatentsView, File Wrapper, PTAB)",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test:patentsview": "tsx examples/test-patentsview.ts",
    "test:filewrapper": "tsx examples/test-file-wrapper.ts",
    "test:ptab": "tsx examples/test-ptab.ts",
    "test:all": "npm run test:patentsview && npm run test:filewrapper && npm run test:ptab",
    "example:analysis": "tsx examples/comprehensive-patent-analysis.ts",
    "example:broadcom": "tsx examples/broadcom-portfolio-builder.ts",
    "download:streaming": "tsx examples/streaming-video-downloader.ts",
    "download:streaming:test": "tsx examples/streaming-video-downloader.ts --limit 100",
    "build": "tsc"
  },
  "keywords": [
    "uspto",
    "patents",
    "patentsview",
    "file-wrapper",
    "ptab",
    "ipr",
    "patent-prosecution",
    "patent-data",
    "api-client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
