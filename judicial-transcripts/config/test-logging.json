{
  "inputDir": "./test",
  "outputDir": "./output/test",
  "logLevel": "info",
  "batchSize": 100,
  "enableElasticSearch": false,
  "logging": {
    "profile": "AppendDatetime",
    "profiles": {
      "Default": {
        "appendTimestamp": false,
        "timestampFormat": "",
        "logLevel": "info",
        "enableWarningLog": true,
        "logDirectory": "logs"
      },
      "AppendDatetime": {
        "appendTimestamp": true,
        "timestampFormat": "YYYY-MM-DD-HHmmss",
        "logLevel": "info",
        "enableWarningLog": true,
        "logDirectory": "logs"
      }
    }
  }
}