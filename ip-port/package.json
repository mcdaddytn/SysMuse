{
  "name": "ip-port",
  "version": "1.0.0",
  "description": "TypeScript clients for USPTO Patent Data APIs (PatentsView, File Wrapper, PTAB)",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test:patentsview": "ts-node --esm examples/test-patentsview.ts",
    "test:filewrapper": "ts-node --esm examples/test-file-wrapper.ts",
    "test:ptab": "ts-node --esm examples/test-ptab.ts",
    "test:all": "npm run test:patentsview && npm run test:filewrapper && npm run test:ptab",
    "example:analysis": "ts-node --esm examples/comprehensive-patent-analysis.ts",
    "example:broadcom": "ts-node --esm examples/broadcom-portfolio-builder.ts",
    "build": "tsc"
  },
  "keywords": [
    "uspto",
    "patents",
    "patentsview",
    "file-wrapper",
    "ptab",
    "ipr",
    "patent-prosecution",
    "patent-data",
    "api-client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  }
}
